#!/usr/bin/env python3
# -*- coding: utf-8 -*-

"""
PostgreSQL loader (accelerated) with:
- PostGIS POINTZ geometry (+ GIST) for coordinates
- FTS (tsvector) + trigger (+ GIN) for metadata
- Fast annotations_terms via NumPy + COPY
- Optional annotations_json aggregation (+ GIN) via --enable-json

Default schema: ns
"""

import argparse
import os
import io
import re
from typing import List, Tuple

import numpy as np
import pandas as pd
from sqlalchemy import create_engine, text
from sqlalchemy.engine import Engine


# -----------------------------
# Args
# -----------------------------
def parse_args():
    ap =3D argparse.ArgumentParser(description=3D"PostgreSQL loader with Po=
stGIS + FTS and accelerated annotations COPY.")
    ap.add_argument("--url", required=3DTrue, help=3D"SQLAlchemy DB URL (po=
stgresql://user:pass@host/db)")
    ap.add_argument("--data-dir", default=3D"./", help=3D"Directory contain=
ing Parquet files")
    ap.add_argument("--schema", default=3D"ns", help=3D"Target schema (defa=
ult: ns)")
    ap.add_argument("--if-exists", choices=3D["replace", "append"], default=
=3D"replace", help=3D"Behavior for coordinates/metadata")
    ap.add_argument("--batch-cols", type=3Dint, default=3D150, help=3D"term=
s_* columns to melt per batch (smaller uses less RAM)")
    ap.add_argument("--stage-chunksize", type=3Dint, default=3D50000, help=
=3D"pandas.to_sql() chunksize for staging loads")
    ap.add_argument("--enable-json", action=3D"store_true", help=3D"Also bu=
ild annotations_json (slow)")
    ap.add_argument("--srid", type=3Dint, default=3D4326, help=3D"SRID for =
geometry(POINTZ). Default 4326")
    return ap.parse_args()


# -----------------------------
# Helpers
# -----------------------------
def load_parquet(path: str) -&gt; pd.DataFrame:
    if not os.path.exists(path):
        raise FileNotFoundError(path)
    return pd.read_parquet(path)


def ensure_schema(engine: Engine, schema: str):
    with engine.begin() as conn:
        conn.execute(text(f"CREATE SCHEMA IF NOT EXISTS {schema};"))
        conn.execute(text(f"SET search_path TO {schema}, public;"))


def ensure_extensions(engine: Engine):
    with engine.begin() as conn:
        conn.execute(text("CREATE EXTENSION IF NOT EXISTS postgis;"))
        conn.execute(text("CREATE EXTENSION IF NOT EXISTS pg_trgm;"))
        conn.execute(text("CREATE EXTENSION IF NOT EXISTS unaccent;"))


def is_finite_series(s: pd.Series) -&gt; pd.Series:
    s =3D pd.to_numeric(s, errors=3D"coerce").astype(float)
    return np.isfinite(s.to_numpy(copy=3DFalse))


# -----------------------------
# Coordinates (POINTZ + GIST)
# -----------------------------
def build_coordinates(engine: Engine, df: pd.DataFrame, schema: str, chunks=
ize: int, if_exists: str, srid: int):
    print("=E2=86=92 coordinates: preparing dataframe")
    must_have =3D ["study_id", "x", "y", "z"]
    missing =3D [c for c in must_have if c not in df.columns]
    if missing:
        raise KeyError(f"coordinates missing columns: {missing}")
    df =3D df[must_have].copy()
    df["study_id"] =3D df["study_id"].astype(str)
    for c in ["x", "y", "z"]:
        df[c] =3D pd.to_numeric(df[c], errors=3D"coerce")
    finite_mask =3D is_finite_series(df["x"]) &amp; is_finite_series(df["y"=
]) &amp; is_finite_series(df["z"])
    bad =3D (~finite_mask).sum()
    if bad:
        print(f"   =E2=80=A6 dropping {bad:,} non-finite rows from coordina=
tes")
    df =3D df[finite_mask].reset_index(drop=3DTrue)

    with engine.begin() as conn:
        if if_exists =3D=3D "replace":
            conn.execute(text(f"DROP TABLE IF EXISTS {schema}.coordinates C=
ASCADE;"))
            conn.execute(text(f"DROP TABLE IF EXISTS {schema}.coordinates_s=
tage CASCADE;"))
        conn.execute(text(f"""
            CREATE UNLOGGED TABLE IF NOT EXISTS {schema}.coordinates_stage =
(
                study_id TEXT NOT NULL,
                x DOUBLE PRECISION NOT NULL,
                y DOUBLE PRECISION NOT NULL,
                z DOUBLE PRECISION NOT NULL
            );
        """))

    print("=E2=86=92 coordinates: loading staging (to_sql)")
    df.to_sql("coordinates_stage", engine, schema=3Dschema, if_exists=3D"ap=
pend", index=3DFalse,
              chunksize=3Dchunksize, method=3D"multi")

    with engine.begin() as conn:
        conn.execute(text(f"""
            CREATE TABLE IF NOT EXISTS {schema}.coordinates (
                study_id TEXT NOT NULL,
                geom geometry(POINTZ, {srid}) NOT NULL
            );
        """))
        if if_exists =3D=3D "replace":
            conn.execute(text(f"TRUNCATE TABLE {schema}.coordinates;"))
        print("=E2=86=92 coordinates: populating geometry from staging")
        conn.execute(text(f"""
            INSERT INTO {schema}.coordinates (study_id, geom)
            SELECT study_id,
                   ST_SetSRID(ST_MakePoint(x, y, z), {srid})::geometry(POIN=
TZ, {srid})
            FROM {schema}.coordinates_stage;
        """))
        print("=E2=86=92 coordinates: indexing &amp; analyze")
        conn.execute(text(f"CREATE INDEX IF NOT EXISTS idx_coordinates_stud=
y ON {schema}.coordinates (study_id);"))
        conn.execute(text(f"CREATE INDEX IF NOT EXISTS idx_coordinates_geom=
_gist ON {schema}.coordinates USING GIST (geom);"))
        conn.execute(text(f"ANALYZE {schema}.coordinates;"))
        conn.execute(text(f"DROP TABLE IF EXISTS {schema}.coordinates_stage=
;"))
    print("=E2=86=92 coordinates (POINTZ + GIST) done.")


# -----------------------------
# Metadata (FTS tsvector + trigger + GIN)
# -----------------------------
def build_metadata(engine: Engine, df: pd.DataFrame, schema: str, if_exists=
: str):
    print("=E2=86=92 metadata: preparing &amp; creating table")
    df =3D df.copy()
    df.columns =3D [str(c).strip().lower().replace(" ", "_") for c in df.co=
lumns]

    with engine.begin() as conn:
        if if_exists =3D=3D "replace":
            conn.execute(text(f"DROP TABLE IF EXISTS {schema}.metadata CASC=
ADE;"))
        cols =3D []
        for c in df.columns:
            if pd.api.types.is_numeric_dtype(df[c]):
                cols.append(f"{c} DOUBLE PRECISION")
            else:
                cols.append(f"{c} TEXT")
        cols.append("fts tsvector")
        conn.execute(text(f"CREATE TABLE IF NOT EXISTS {schema}.metadata ({=
', '.join(cols)});"))

    print("=E2=86=92 metadata: bulk inserting (to_sql)")
    df.to_sql("metadata", engine, schema=3Dschema, if_exists=3D"append", in=
dex=3DFalse, chunksize=3D20000, method=3D"multi")

    with engine.begin() as conn:
        res =3D conn.execute(text(f"""
            SELECT column_name, data_type FROM information_schema.columns
            WHERE table_schema =3D :schema AND table_name =3D 'metadata';
        """), {"schema": schema}).fetchall()
        text_cols =3D [r[0] for r in res if r[0] !=3D "fts" and r[1] in ("t=
ext", "character varying")]
        if text_cols:
            cols_expr =3D " || ' ' || ".join([f"coalesce({c},'')" for c in =
text_cols])
            print("=E2=86=92 metadata: computing tsvector over text columns=
")
            conn.execute(text(f"UPDATE {schema}.metadata SET fts =3D to_tsv=
ector('pg_catalog.english', {cols_expr});"))
            print("=E2=86=92 metadata: creating GIN index &amp; trigger")
            conn.execute(text(f"CREATE INDEX IF NOT EXISTS idx_metadata_fts=
 ON {schema}.metadata USING GIN (fts);"))
            conn.execute(text(f"DROP TRIGGER IF EXISTS metadata_fts_update =
ON {schema}.metadata;"))
            conn.execute(text(f"""
                CREATE TRIGGER metadata_fts_update
                BEFORE INSERT OR UPDATE ON {schema}.metadata
                FOR EACH ROW EXECUTE FUNCTION tsvector_update_trigger(
                    'fts', 'pg_catalog.english', {", ".join(text_cols)}
                );
            """))
            conn.execute(text(f"ANALYZE {schema}.metadata;"))
    print("=E2=86=92 metadata (FTS + trigger) done.")


# -----------------------------
# Annotations -&gt; sparse terms via NumPy + COPY (+ optional JSONB)
# -----------------------------
def copy_terms(engine: Engine, schema: str, rows: List[Tuple[str, str, str,=
 float]]):
    buf =3D io.StringIO()
    for study_id, contrast_id, term, weight in rows:
        # COPY text format uses \N for NULL
        cval =3D r'\N' if contrast_id is None else str(contrast_id)
        buf.write(f"{study_id}\t{cval}\t{term}\t{weight}\n")
    buf.seek(0)
    raw =3D engine.raw_connection()
    try:
        with raw.cursor() as cur:
            cur.execute("SET LOCAL synchronous_commit =3D off;")
            cur.copy_expert(
                f"COPY {schema}.annotations_terms (study_id, contrast_id, t=
erm, weight) FROM STDIN WITH (FORMAT text)",
                buf,
            )
        raw.commit()
    finally:
        raw.close()


def build_annotations(engine: Engine, df: pd.DataFrame, schema: str, batch_=
cols: int, enable_json: bool =3D False):
    print("=E2=86=92 annotations: preparing")
    fixed =3D {"id", "study_id", "contrast_id"}
    term_cols =3D [c for c in df.columns if c not in fixed and str(c).start=
swith("terms_")]
    if not term_cols:
        raise RuntimeError("No term columns found in annotations.*")

    with engine.begin() as conn:
        conn.execute(text(f"DROP TABLE IF EXISTS {schema}.annotations_terms=
 CASCADE;"))
        conn.execute(text(f"""
            CREATE UNLOGGED TABLE {schema}.annotations_terms (
                study_id    TEXT NOT NULL,
                contrast_id TEXT,
                term        TEXT NOT NULL,
                weight      DOUBLE PRECISION NOT NULL
            );
        """))
        if enable_json:
            conn.execute(text(f"DROP TABLE IF EXISTS {schema}.annotations_j=
son CASCADE;"))
            conn.execute(text(f"""
                CREATE TABLE {schema}.annotations_json (
                    study_id    TEXT NOT NULL,
                    contrast_id TEXT,
                    terms       JSONB NOT NULL,
                    PRIMARY KEY (study_id, contrast_id)
                );
            """))

    id_vars =3D ["study_id", "contrast_id"]
    total_inserted =3D 0

    # Pre-extract ID arrays once
    sid_arr =3D df["study_id"].astype(str).to_numpy(copy=3DFalse)
    cid_series =3D df["contrast_id"] if "contrast_id" in df.columns else pd=
.Series([None]*len(df))
    cid_arr =3D cid_series.where(pd.notna(cid_series), None).astype(object)=
.to_numpy(copy=3DFalse)

    for i in range(0, len(term_cols), batch_cols):
        cols =3D term_cols[i:i+batch_cols]

        # Filter out columns that are entirely null or &lt;=3D 0 to avoid u=
seless processing
        nonempty =3D [c for c in cols if (pd.to_numeric(df[c], errors=3D"co=
erce") &gt; 0).any()]
        if not nonempty:
            continue

        # Build rows with NumPy (avoid huge melted DataFrame)
        term_rows: List[Tuple[str, str, str, float]] =3D []
        for c in nonempty:
            col =3D pd.to_numeric(df[c], errors=3D"coerce").to_numpy(copy=
=3DFalse)
            mask =3D np.isfinite(col) &amp; (col &gt; 0)
            if not mask.any():
                continue
            idx =3D np.nonzero(mask)[0]
            term =3D re.sub(r"^terms_[^_]*__", "", str(c)).strip().lower()
            term_rows.extend(zip(sid_arr[idx], cid_arr[idx], [term]*len(idx=
), col[idx].astype(float)))

        if term_rows:
            copy_terms(engine, schema, term_rows)
            total_inserted +=3D len(term_rows)
            print(f"   =E2=80=A6 copied {len(term_rows):,} rows (cumulative=
 {total_inserted:,})")

    # Indexes AFTER bulk load (faster)
    with engine.begin() as conn:
        conn.execute(text(f"CREATE INDEX IF NOT EXISTS idx_annotations_term=
s_term ON {schema}.annotations_terms (term);"))
        conn.execute(text(f"CREATE INDEX IF NOT EXISTS idx_annotations_term=
s_study ON {schema}.annotations_terms (study_id);"))
        conn.execute(text(f"CREATE INDEX IF NOT EXISTS idx_annotations_term=
s_term_study ON {schema}.annotations_terms (term, study_id);"))
        conn.execute(text(f"ANALYZE {schema}.annotations_terms;"))
        # Build PK/unique AFTER load to avoid per-row maintenance
        conn.execute(text(f"CREATE UNIQUE INDEX IF NOT EXISTS ux_annotation=
s_terms ON {schema}.annotations_terms (study_id, contrast_id, term);"))
        conn.execute(text(f"ALTER TABLE {schema}.annotations_terms ADD CONS=
TRAINT pk_annotations_terms PRIMARY KEY USING INDEX ux_annotations_terms;")=
)

        if enable_json:
            print("=E2=86=92 annotations_json: aggregating (this may take a=
 while)")
            conn.execute(text("SET LOCAL work_mem =3D '512MB';"))
            conn.execute(text("SET LOCAL maintenance_work_mem =3D '1GB';"))
            conn.execute(text(f"""
                INSERT INTO {schema}.annotations_json (study_id, contrast_i=
d, terms)
                SELECT study_id, contrast_id, jsonb_object_agg(term, weight=
)
                FROM {schema}.annotations_terms
                GROUP BY study_id, contrast_id
                ON CONFLICT (study_id, contrast_id) DO UPDATE
                SET terms =3D EXCLUDED.terms;
            """))
            conn.execute(text(f"CREATE INDEX IF NOT EXISTS idx_annotations_=
json_terms_gin ON {schema}.annotations_json USING GIN (terms);"))
            conn.execute(text(f"ANALYZE {schema}.annotations_json;"))

    print(f"=E2=86=92 annotations_terms total inserted: {total_inserted:,}"=
)
    if enable_json:
        print("   =E2=80=A6 annotations_json populated and indexed.")
    print("   =E2=80=A6 annotations done.")


