pace-pre-wrap">import json
import argparse
from urllib.parse import urlparse, parse_qsl, urlencode, urlunparse

from sqlalchemy import create_engine, text
from sqlalchemy.exc import SQLAlchemyError

def ensure_sslmode_required(db_url: str) -&gt; str:
    """
    Ensure ?sslmode=3Drequire is present in the connection URL.
    """
    parsed =3D urlparse(db_url)
    q =3D dict(parse_qsl(parsed.query, keep_blank_values=3DTrue))
    if q.get("sslmode") is None:
        q["sslmode"] =3D "require"
    new_query =3D urlencode(q)
    return urlunparse(parsed._replace(query=3Dnew_query))

def run(conn, sql, name, summary, key):
    """
    Execute a SQL statement and print pass/fail.
    On failure, issue a ROLLBACK so subsequent statements can proceed.
    Returns (ok: bool, rows: list|None).
    """
    try:
        res =3D conn.execute(text(sql))
        rows =3D []
        try:
            rows =3D res.fetchall()
        except Exception:
            # Statement did not return rows (e.g., CREATE EXTENSION)
            rows =3D []
        print(f"=E2=9C=85 {name}: OK")
        if rows:
            print("   =E2=86=B3 Result:", rows[0])
        summary[key] =3D {"ok": True, "result": rows[:1]}
        return True, rows
    except SQLAlchemyError as e:
        print(f"=E2=9D=8C {name}: FAILED")
        print(f"   =E2=86=B3 Error: {e}")
        summary[key] =3D {"ok": False, "error": str(e)}
        try:
            # Clear aborted transaction state
            conn.exec_driver_sql("ROLLBACK")
        except Exception:
            pass
        return False, None

def ensure_extension(conn, extname: str, summary, key_prefix: str):
    """
    Try to enable a PostgreSQL extension in the *current database*.
    Then verify presence via pg_extension.
    """
    run(conn, f"CREATE EXTENSION IF NOT EXISTS {extname};",
        f"enable {extname}", summary, f"{key_prefix}.enable")

    # Verify it's installed
    ok, rows =3D run(conn,
        f"SELECT extname, extversion, extnamespace::regnamespace "
        f"FROM pg_extension WHERE extname =3D '{extname}';",
        f"{extname} extension installed", summary, f"{key_prefix}.installed=
")

    return ok

def check_tsvector(conn, summary):
    print("\n=3D=3D=3D Check tsvector (Full-Text Search) =3D=3D=3D")
    run(conn,
        "SELECT EXISTS (SELECT 1 FROM pg_type WHERE typname =3D 'tsvector')=
 AS has_tsvector;",
        "tsvector type exists", summary, "tsvector.type_exists")
    run(conn,
        "SELECT to_tsvector('english', 'hello world') AS tv;",
        "to_tsvector works", summary, "tsvector.to_tsvector")

def check_pgvector(conn, summary):
    print("\n=3D=3D=3D Check pgvector (Vector similarity) =3D=3D=3D")
    ensure_extension(conn, "vector", summary, "pgvector")

