from flask import Flask, jsonify, abort, send_file
import os
from sqlalchemy import create_engine, text

_engine =3D None

def get_engine():
    global _engine
    if _engine is not None:
        return _engine
    db_url =3D os.getenv("DB_URL")
    if not db_url:
        raise RuntimeError("Missing DB_URL environment variable.")
    if db_url.startswith("postgres://"):
        db_url =3D "postgresql://" + db_url[len("postgres://"):]
    _engine =3D create_engine(db_url, pool_pre_ping=3DTrue)
    return _engine

def create_app():
    app =3D Flask(__name__)

    @app.get("/", endpoint=3D"health")
    def health():
        return "&lt;p&gt;Server working!&lt;/p&gt;"

    @app.get("/test_db", endpoint=3D"test_db")
    def test_db():
        eng =3D get_engine()
        payload =3D {"ok": False, "dialect": eng.dialect.name}

        try:
            with eng.begin() as conn:
                conn.execute(text("SET search_path TO ns, public;"))
                payload["version"] =3D conn.exec_driver_sql("SELECT version=
()").scalar()
                payload["coordinates_count"] =3D conn.execute(text("SELECT =
COUNT(*) FROM ns.coordinates")).scalar()
                payload["metadata_count"] =3D conn.execute(text("SELECT COU=
NT(*) FROM ns.metadata")).scalar()
                payload["annotations_terms_count"] =3D conn.execute(text("S=
ELECT COUNT(*) FROM ns.annotations_terms")).scalar()

                rows =3D conn.execute(text(
                    "SELECT study_id, ST_X(geom) AS x, ST_Y(geom) AS y, ST_=
Z(geom) AS z FROM ns.coordinates LIMIT 3"
                )).mappings().all()
                payload["coordinates_sample"] =3D [dict(r) for r in rows]

                rows =3D conn.execute(text("SELECT * FROM ns.metadata LIMIT=
 3")).mappings().all()
                payload["metadata_sample"] =3D [dict(r) for r in rows]

                rows =3D conn.execute(text(
                    "SELECT study_id, contrast_id, term, weight FROM ns.ann=
otations_terms LIMIT 3"
                )).mappings().all()
                payload["annotations_terms_sample"] =3D [dict(r) for r in r=
ows]

            payload["ok"] =3D True
            return jsonify(payload), 200
        except Exception as e:
            payload["error"] =3D str(e)
            return jsonify(payload), 500

    return app

app =3D create_app()

